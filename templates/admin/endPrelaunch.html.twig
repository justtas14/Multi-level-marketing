{% extends 'base.html.twig' %}

{% block title %}End Prelaunch{% endblock %}

{% block body %}

    <div id="flash-messages">
        {% for message in app.flashes('error') %}
            <div class="alert alert-error">
                {{ message }}
            </div>
        {% endfor %}
        {% for message in app.flashes('success') %}
            <div class="alert alert-success">
                {{ message }}
            </div>
        {% endfor %}
    </div>

    <main id="main">
        {{ form_start(form) }}
        {{ form_widget(form) }}
        {{ form_end(form) }}
    </main>

{% endblock %}
