{% extends 'bodyBase.html.twig' %}

{% block title %}User Search{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script>
        window.mainAction = function (id) {
            window.location.href = '/admin/users/' + id;
        };
        window.mainActionLabel = 'Details';
    </script>
    <script type="text/javascript" src="{{ asset('build/app.js') }}"></script>
{% endblock %}

{% block content %}
    <div class="admin-contentContainer">
        <div class="card">
            <div class="card-content">
                <span class="card-title">User search</span>
            <div id="root"></div>
            </div>
        </div>
    </div>
{% endblock %}