<div class="sidebar-logoContainer">
    {% set configuration = getConfiguration() %}
    {#<a href="{{ path('home') }}">#}
    {% if configuration.mainLogo is not null %}
        <img class="sidebar-logo" src="{{ configuration.mainLogo|downloadUrlParser }}" />
    {% else%}
        <img class="sidebar-logo" src="{{ asset('assets/images/plum_tree_logo.png') }}" />
    {% endif %}
    {#</a>#}
    <div class="hamburger hamburger--collapse">
        <div class="hamburger-box">
            <div class="hamburger-inner"></div>
        </div>
    </div>
</div>


